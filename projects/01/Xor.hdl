/**
 *  Exclusive-or gate:
 *  out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=Nota);
    Not(in=b, out=Notb);
    And(a=a, b=Notb, out=w1);
    And(a=Nota, b=b, out=w2);
    Or(a=w1, b=w2, out=out);
}
